{
  "name": "Check",
  "version": "0.0.1",
  "description": "Verify breaking news online",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/meedan/checkdesk-client.git"
  },
  "scripts": {
    "logo": "cd src/assets/images/logo && ./generate.sh && cd ..",
    "style": "node-sass src/app/styles/stylesheet.scss --output-style=compressed | postcss --use autoprefixer -o build/web/css/stylesheet.css && cp -r src/app/styles/images build/web/",
    "style:rebuild": "npm rebuild node-sass && mkdir -p build/web/css 2>/dev/null && npm run style",
    "build": "rm -rf build/web && npm run style && npm run build:gulp",
    "build:gulp": "BABEL_ENV=production gulp build:web",
    "build:test": "rm -rf build/web && npm run style && npm run build:test:gulp",
    "build:test:gulp": "BABEL_ENV=production gulp build:web:test",
    "watch:styles": "watch 'npm run style' ./src/app/styles",
    "publish": "node scripts/server.js",
    "test": "cd test && rspec && cd ..",
    "test:ff": "cd test && rspec --fail-fast && cd ..",
    "lint:styles": "sass-lint --config .sass-lint.yml './**/*.scss' --verbose --ignore './test/**/*.scss, ./node_modules/**/*.scss'"
  },
  "keywords": [
    "react",
    "reactjs",
    "redux",
    "checkmedia"
  ],
  "authors": [
    "Meedan"
  ],
  "license": "MIT",
  "devDependencies": {
    "chromedriver": "2.24.0",
    "eslint": "^1.7.1",
    "eslint-config-airbnb": "^0.1.0",
    "eslint-plugin-react": "^3.2.3",
    "expect": "^1.12.2",
    "legit-tests": "^0.6.1",
    "mocha": "^2.3.3",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.0.1",
    "redux-devtools": "^3.0.0-beta-3",
    "redux-devtools-dock-monitor": "^1.0.0-beta-3",
    "redux-devtools-log-monitor": "^1.0.0-beta-3",
    "redux-immutable-state-invariant": "^1.1.0",
    "redux-logger": "^2.0.4",
    "selenium-webdriver": "^2.48.0",
    "watch": "^0.19.1",
    "webpack-dev-server": "^1.12.1"
  },
